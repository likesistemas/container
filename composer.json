{
    "name": "like/psr-container",
    "type": "library",
    "description": "Implements the PSR-11 interface using `illuminate/container`.",
    "require": {
        "php": ">=5.6",
        "illuminate/container": "^5.0 || ^8.0 || ^9.0",
        "psr/container": "=1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Container\\Psr11\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Container\\Psr11\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Like Sistemas",
            "email": "dev@likesistemas.com.br",
            "homepage": "https://www.likesistemas.com.br/"
        }
    ],
    "scripts": {
        "fix": "php-cs-fixer fix",
        "fix:ci": "php-cs-fixer fix --dry-run --stop-on-violation",
        "test": "phpunit",
        "analyse": "phpstan analyse"
    }
}
